<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ecsite.mapper.ProductMapper">
    <select id="findAll"
        resultType="com.example.ecsite.model.Product">
        SELECT id, name, description, price, imageurl,is_in_cart
        FROM products
    </select>
</mapper>